-static bool ecezo_data_available(const ecezo_context dev, unsigned int millis)
-{
-    assert(dev != NULL);
-
-    // i2c, we don't support this
-    if (dev->i2c)
-    {
-        return false;
-    }
-
-    // uart
-    if (mraa_uart_data_available(dev->uart, millis))
-        return true;
-    else
-        return false;
-}
