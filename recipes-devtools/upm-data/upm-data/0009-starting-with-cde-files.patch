-   if (scaledResistance <= DS1808_MAX_POT_VALUE)
-      result = 0;
-   else {
-      result = scaledResistance - DS1808_MAX_POT_VALUE;
-   }
-
-   if (result > DS1808_MAX_POT_VALUE)
-      result = DS1808_MAX_POT_VALUE;
