-void ds18b20_close(ds18b20_context dev)
-{
-    assert(dev != NULL);
-
-    if (dev->devices)
-        free(dev->devices);
-    if (dev->ow)
-        mraa_uart_ow_stop(dev->ow);
-
-    free(dev);
-}
