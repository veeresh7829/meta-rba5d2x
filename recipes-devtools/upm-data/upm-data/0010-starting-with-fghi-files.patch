-    if((x >= _width) || (y >= _height)) {
-        return;
-    }
-    
-    if((y+h-1) >= _height) {
-        h = _height-y;
-    }
-
-    setAddrWindow(x, y, x, y+h-1);
-
-    uint8_t hi = color >> 8;
-    uint8_t lo = color;
