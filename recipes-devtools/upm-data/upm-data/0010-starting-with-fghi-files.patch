-  // wait for the device to report ready
-  waitforDeviceReady();
-
-  // start conversion, PT only
-  uint8_t cmd = CMD_ADC_CVT | (CHNL_PT << CHNL_SHIFT) | (m_dsr << DSR_SHIFT);
