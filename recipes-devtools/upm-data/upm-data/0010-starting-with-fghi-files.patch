-  high = m_i2c.readReg(REG_OUT_Y_H);
-  m_rawY = ((high << 8) | low);
-
-  // Z
-  low = m_i2c.readReg(REG_OUT_Z_L);
-  high = m_i2c.readReg(REG_OUT_Z_H);
-  m_rawZ = ((high << 8) | low);
