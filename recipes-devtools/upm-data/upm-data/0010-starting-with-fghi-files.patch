-      
-      m_powerFactorPhaseA       = regs2float(buf[22], buf[23]);
-      m_powerFactorPhaseB       = regs2float(buf[24], buf[25]);
-      m_powerFactorPhaseC       = regs2float(buf[26], buf[27]);
