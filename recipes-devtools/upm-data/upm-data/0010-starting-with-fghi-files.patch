-bool HMTRP::reset()
-{
-  char pkt[3];
-
-  pkt[0] = HMTRP_START1;
-  pkt[1] = HMTRP_START2;
-  pkt[2] = RESET;
-
-  writeData(pkt, 3);
-
-  return checkOK();
-}
