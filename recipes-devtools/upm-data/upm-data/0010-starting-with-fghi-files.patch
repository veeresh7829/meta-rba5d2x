-bool HMTRP::setFrequency(uint32_t freq)
-{
-  char pkt[7];
-
-  pkt[0] = HMTRP_START1;
-  pkt[1] = HMTRP_START2;
-  pkt[2] = SET_FREQUENCY;
-
-  pkt[3] = ( ((freq & 0xff000000) >> 24) & 0xff );
-  pkt[4] = ( ((freq & 0x00ff0000) >> 16) & 0xff );
-  pkt[5] = ( ((freq & 0x0000ff00) >> 8)  & 0xff );
-  pkt[6] = ( (freq & 0x000000ff) & 0xff );
-
-  writeData(pkt, 7);
-
-  return checkOK();
-}
