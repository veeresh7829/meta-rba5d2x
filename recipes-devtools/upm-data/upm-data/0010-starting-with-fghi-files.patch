-void GFX::drawLine(int16_t x0, 
-                   int16_t y0, 
-                   int16_t x1, 
-                   int16_t y1, 
-                   uint16_t color) {
-                   
-    int16_t steep = abs(y1 - y0) > abs(x1 - x0);
-    if (steep) {
-        adagfxswap(x0, y0);
-        adagfxswap(x1, y1);
-    }
-
-    if (x0 > x1) {
-        adagfxswap(x0, x1);
-        adagfxswap(y0, y1);
-    }
-
-    int16_t dx, dy;
-    dx = x1 - x0;
-    dy = abs(y1 - y0);
-
-    int16_t err = dx / 2;
-    int16_t ystep;
-
-    if (y0 < y1) {
-        ystep = 1;
-    } else {
-        ystep = -1;
-    }
-
-    for (; x0<=x1; x0++) {
-        if (steep) {
-            drawPixel(y0, x0, color);
-        } else {
-            drawPixel(x0, y0, color);
-        }
-        err -= dy;
-        if (err < 0) {
-            y0 += ystep;
-          err += dx;
-        }
-    }
-}
